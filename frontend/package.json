{
  "name": "frontend",
  "private": true,
  "scripts": {
    "dev": "vite --mode dev --host 0.0.0.0 --port 5173 --strictPort",
    "dev:force": "vite --host 0.0.0.0 --port 5173 --force",
    "clean:vite": "node -e \"require('fs').rmSync('node_modules/.vite', { recursive: true, force: true })\"",
    "biome:check": "biome check .",
    "biome:fix": "biome check . --apply",
    "biome:fix:unsafe": "biome check . --apply-unsafe"
  },
  "dependencies": {
    "@azure/msal-browser": "^3.30.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.6.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "typescript": "^5.6.0",
    "vite": "^5.4.8"
  }
}
